{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "kafka_exporter datasource",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "8.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "title": "Kafka Exporter (Lag compatible)",
  "uid": "kexp-lag-compat",
  "timezone": "",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "style": "dark",
  "refresh": "30s",
  "panels": [
    {
      "type": "row",
      "title": "All Consumer Group Lag",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Consumer Group Max Lag Seconds",
      "id": 2,
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(50, max by (consumergroup) ( sum by (consumergroup, topic, partition) (kafka_consumergroup_lag{namespace=\"$namespace\",consumergroup=~\"$consumer_group\"}) / clamp_min(rate(kafka_topic_partition_current_offset{namespace=\"$namespace\"}[5m]), 1) ))",
          "legendFormat": "{{consumergroup}}"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "none" } },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 1, "showPoints": "never", "spanNulls": false }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Consumer Group Lag Partition Seconds",
      "id": 3,
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(25, (kafka_consumergroup_lag{namespace=\"$namespace\",consumergroup=~\"$consumer_group\"}) / clamp_min(rate(kafka_topic_partition_current_offset{namespace=\"$namespace\"}[5m]), 1))",
          "legendFormat": "{{consumergroup}},{{topic}},{{partition}}"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "none" } },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 1, "showPoints": "never", "spanNulls": false }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Consumer Group Max Lag Offsets",
      "id": 4,
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 11 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(50, max by (consumergroup) (kafka_consumergroup_lag{namespace=\"$namespace\",consumergroup=~\"$consumer_group\"}))",
          "legendFormat": "{{consumergroup}}"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "none" } },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 1, "showPoints": "never", "spanNulls": false }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Consumer Group Lag Partition Offsets",
      "id": 5,
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 11 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(25, kafka_consumergroup_lag{namespace=\"$namespace\",consumergroup=~\"$consumer_group\"})",
          "legendFormat": "{{consumergroup}},{{topic}},{{partition}}"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "none" } },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 1, "showPoints": "never", "spanNulls": false }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Throughput",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 21 },
      "id": 6,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Message in per second",
      "id": 7,
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 22 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(kafka_topic_partition_current_offset{namespace=\"$namespace\",instance=\"$instance\",topic=~\"$topic\"}[1m])) by (topic)",
          "legendFormat": "{{topic}}"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "none" } },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 1, "showPoints": "never", "spanNulls": false }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Message consume per minute",
      "id": 8,
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 22 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(delta(kafka_consumergroup_current_offset{namespace=\"$namespace\",instance=~\"$instance\",topic=~\"$topic\"}[5m])/5) by (consumergroup, topic)",
          "legendFormat": "{{consumergroup}} (topic: {{topic}})"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "none" } },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "custom": { "drawStyle": "line", "lineWidth": 1, "showPoints": "never", "spanNulls": false }
        },
        "overrides": []
      }
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "query": "prometheus",
        "hide": 0
      },
      {
        "type": "query",
        "name": "namespace",
        "label": "Namespace",
        "hide": 0,
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 1,
        "query": "label_values(kafka_consumergroup_lag, namespace)"
      },
      {
        "type": "query",
        "name": "instance",
        "label": "Instance",
        "hide": 0,
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 1,
        "query": "label_values(kafka_consumergroup_lag{namespace=\"$namespace\"}, instance)"
      },
      {
        "type": "query",
        "name": "consumer_group",
        "label": "Consumer Group",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 1,
        "query": "label_values(kafka_consumergroup_lag{namespace=\"$namespace\"}, consumergroup)"
      },
      {
        "type": "query",
        "name": "topic",
        "label": "Topic",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 1,
        "query": "label_values(kafka_topic_partition_current_offset{namespace=\"$namespace\",instance=\"$instance\"}, topic)"
      }
    ]
  },
  "time": { "from": "now-30m", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["10s","30s","1m","5m","15m","30m","1h"],
    "time_options": ["5m","15m","1h","6h","12h","24h","7d","30d"]
  },
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "builtIn": 1,
        "name": "Annotations & Alerts",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "datasource": { "type": "datasource", "uid": "grafana" }
      }
    ]
  }
}